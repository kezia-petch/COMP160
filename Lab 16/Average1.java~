/**Average class
  * Lab 16 COMP160
  */
public class Average1{
  public static void main(String[]args){
    int [] [] table = {{1, 2, 3}, {4, 5, 6}, {7, 8}};
    
    int count = 0;
    for (int [] row : table){
      for (int col : row){
        count += col;
        System.out.print(col + '#');
      }
      System.out.println("\tAverage : " + (count/row.length));
    }
  }
}
